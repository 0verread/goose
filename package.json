{
  "name": "goose",
  "displayName": "goose",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:goose-left-panel"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "goose-left-chatView",
          "title": "Goose",
          "icon": "assets/goose-logo.svg"
        }
      ]
    },
    "views": {
      "goose-left-chatView": [
        {
          "type": "webview",
          "id": "goose-left-panel",
          "name": "Goose",
          "icon": "assets/goose-logo.svg",
          "contextualTitle": ""
        }
      ]
    },
    "commands": [
      {
        "command": "goose.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "goose.explain",
        "title": "Goose: explain"
      },
      {
        "command": "goose.optimize",
        "title": "Goose: optimize"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "goose.explain",
          "when": "editorHasSelection",
          "group": "mygroup@1"
        },
        {
          "command": "goose.optimize",
          "when": "editorHasSelection",
          "group": "mygroup@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w --bundleConfigAsCjs\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@tsconfig/svelte": "^4.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "openai": "^3.3.0",
    "rollup": "^3.25.1",
    "rollup-plugin-svelte": "^7.1.5",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@types/react": "^18.2.9",
    "@types/react-dom": "^17.0.11",
    "dotenv-webpack": "^7.1.0",
    "module-alias": "^2.2.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
